 {
	 "plugins": [
     ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "true" }],
		 [
			 "transform-runtime",
			 {
	           "helpers": false,
	           "polyfill": false,
	           "regenerator": true,
	           "moduleName": "babel-runtime"
	         }
		 ]
	 ],
	 "env": {
		"production": {
			"presets": [
				["env", {
		        	"targets": {
		          		"browsers": ["last 2 versions", "ie > 7"]
		  			}
		      	}],
				"react",
	   		 	"stage-1"
			],
			"plugins": [
				"transform-decorators-legacy",
				"transform-class-properties"
			]
		},
		"development": {
			"presets": [
				["env", {
					"modules": false
				}],
				"react",
	   		 	"stage-1"
			],
			"plugins": [
				"transform-decorators-legacy",
				"transform-class-properties",
				"react-hot-loader/babel"
			]
		},
		"test": {
			"presets": [
				["env"],
				"react",
	   		 	"stage-1"
			],
			"plugins": [
				"transform-decorators-legacy",
				"transform-class-properties",
				[
					"transform-runtime"
				]
			]
		},
	 }
 }
